    {
        "id": "bd773d4aa48cb6b3",
        "type": "function",
        "z": "e94eb63fb6490cfa",
        "name": "prepare email basurero",
        "func": "var rec_msg = msg.payload.toString();\n\nvar msgJson = JSON.parse(rec_msg);\n\nvar measure_value = msgJson.measure_value;\nvar name = msgJson.name;\n\nvar yellow = \"\\nContainer is between 20% and 75%.\\nWe suggest scheduling garbage collection.\";\nvar red = \"\\nContainer is over 75%. \\nWe suggest an immediate garbage collection.\";\n\nif (measure_value === \"error\") {\n    msg.payload = \"error -> \".concat(name);\n}\n\nelse if (measure_value === \"verde\"){\n    msg.payload = \"verde -> \".concat(name);\n}\nelse if(measure_value === \"amarillo\"){\n    msg.topic = \"amarillo -> \".concat(name)\n    msg.payload = \"amarillo -> \".concat(name).concat(yellow);\n}\nelse if(measure_value === \"rojo\"){\n    \"rojo -> \".concat(name);\n    msg.payload = \"rojo -> \".concat(name).concat(red);\n}\nelse {\n    msg.payload = \"null\";\n}\n\nreturn msg;",
