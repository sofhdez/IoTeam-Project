    {
        "id": "844b5a91e9d1a0b8",
        "type": "function",
        "z": "e94eb63fb6490cfa",
        "name": "level-process-message",
        "func": "var topic = msg.topic;\n\nvar rec_msg = msg.payload.toString();\nvar rec_msg_arr = rec_msg.split(\";\");\n\nvar measure_label;\nvar measure_val;\n\nvar time = rec_msg_arr[0].split(\"=\")[1];\nvar measure = Number(rec_msg_arr[1].split(\"=\")[1]);\nvar coord = rec_msg_arr[2].split(\"=\")[1];\n\nvar f_measure = \"meas_\".concat(topic);\nvar o_meas = flow.get(f_measure)||0;\n\nif (o_meas === \"verde\" || o_meas === \"amarillo\" ||  o_meas === \"rojo\" ||  o_meas === \"error\" ){\n    flow.set(f_measure, measure);\n}\nelse if (measure < 0 || measure > 100 ){\n    flow.set(f_measure, \"error\");\n    \n    msg.payload = \n    {    name:topic.split(\"/\")[0],\n    measure_value: flow.get(f_measure),\n    measure_label: \"error\",\n    measure_val: measure,\n    //measure_value: measure,\n    SIDC: \"IOGA--------\",\n    options: {\"fillOpacity\":0.8,\n    \"additionalInformation\":flow.get(f_measure)},\n    lat: Number(coord.split(\":\")[0]),\n    lon: Number(coord.split(\":\")[1])\n    }\n}\nelse if (measure < 20){\n    flow.set(f_measure, \"verde\");\n    \n    msg.payload = \n    {    name:topic.split(\"/\")[0],\n    measure_value: flow.get(f_measure),\n    measure_label: \"verde\",\n    measure_val: measure,\n    //measure_value: measure,\n    SIDC: \"INGC--------\",\n    options: {\"fillOpacity\":0.8,\n    \"additionalInformation\":flow.get(f_measure)},\n    lat: Number(coord.split(\":\")[0]),\n    lon: Number(coord.split(\":\")[1])\n    }\n}\nelse if (measure >= 20 && measure <= 75){\n    flow.set(f_measure, \"amarillo\");\n    \n    msg.payload = \n    {    name:topic.split(\"/\")[0],\n    measure_value: flow.get(f_measure),\n    measure_label: \"amarillo\",\n    measure_val: measure,\n    //measure_measure: measure,\n    SIDC: \"INGD--------\",\n    options: {\"fillOpacity\":0.8,\n    \"additionalInformation\":flow.get(f_measure)},\n    lat: Number(coord.split(\":\")[0]),\n    lon: Number(coord.split(\":\")[1])\n    }\n}\nelse if (measure > 75){\n    flow.set(f_measure, \"rojo\");\n    \n    msg.payload = \n    {    name:topic.split(\"/\")[0],\n    measure_value: flow.get(f_measure),\n    measure_label: \"rojo\",\n    measure_val: measure,\n    //measure_value: measure,\n    SIDC: \"INGX--------\",\n    options: {\"fillOpacity\":0.8,\n    \"additionalInformation\":flow.get(f_measure)},\n    lat: Number(coord.split(\":\")[0]),\n    lon: Number(coord.split(\":\")[1])\n    }\n    \n}\n\nelse{\n    flow.set(f_measure, measure);\n    \n    msg.payload = \n    {    name:topic.split(\"/\")[0],\n    measure_value: flow.get(f_measure),\n    SIDC: \"ENOPA-------\",\n    options: {\"fillOpacity\":0.8,\n    \"additionalInformation\":flow.get(f_measure)},\n    lat: Number(coord.split(\":\")[0]),\n    lon: Number(coord.split(\":\")[1])\n    }\n    \n}\n\n/*\nmsg.payload = \n{    name:topic.split(\"/\")[0],\nmeasure_value: flow.get(f_measure),\nSIDC: \"ENOPA--------\",\noptions: {\"fillOpacity\":0.8,\n\"additionalInformation\":flow.get(f_measure)},\nlat: Number(coord.split(\":\")[0]),\nlon: Number(coord.split(\":\")[1])\n}\n*/\n\nreturn msg;",
